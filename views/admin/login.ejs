<body>
    <main style="height: 90vh; width: 100%; display: flex; justify-content: center; align-items: center;">
        <div>
                <div class="row">
                    <div class="col-md-12 mx-auto">
                        <div class="alert alert-success text-center ">
                            <br>
                            <br>

                            <h1 class="mb-5">Admin login Form</h1>
                            <div class="row">
                                <div class="col-12 text-left">
                                    <form action="/admin/check_admin_login" method="post" autocomplete="off">
                                        <h5 class="mt-2 text-primary">Admin Email <span class="text-danger">*</span></h5>
                                        <input type="email" name="admin_email" class="form-control">
                                        <br>
                                        <h5 class="mt-2 text-primary">Admin Password <span class="text-danger">*</span></h5>
                                        <input type="password" name="admin_password" class="form-control">
                                        <br>
                                        <br>
                                        <button class="btn btn-success btn-block">Login</button>
                                        <br>
                                        <br>
                                    </form>

                                </div>
                            </div>
                        </div>
                    </div>
                </div>
        </div>
    </main>
    <footer class="py-4 bg-light mt-auto" style="position: sticky; bottom: 0; ">
        <div class="container-fluid px-4">
            <div class="d-flex align-items-center justify-content-between small">
                <div class="text-muted">Copyright &copy; #Genius 2023</div>
            </div>
        </div>
    </footer>

