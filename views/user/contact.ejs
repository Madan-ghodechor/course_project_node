<%- include('header.ejs') %>
<div class="container">
    <div class="row" style="height: 100vh;">
        <div class="col-12 alert alert-primary d-flex justify-content-center align-items-center" >
            <h1>Contact Us</h1>
        </div>
        <div class="col-md-12">
            <form action="/save_query" method="post" autocomplete="off">
                <div class="row">
                    <div class="col-md-6 mt-3">
                        <span class="text-danger">*</span>
                        <input type="text" class="form-control" placeholder="Enter First name" name="Qu_Fname" required>
                    </div>
                    <div class="col-md-6 mt-3">
                        <span class="text-danger">*</span>
                        <input type="text" class="form-control" placeholder="Enter Last Name " name="Qu_Lname" required>
                    </div>
                    <div class="col-md-6 mt-3">
                        <span class="text-danger">*</span>
                        <input type="text" class="form-control" placeholder="Enter Mobile Number" name="Qu_mobile" required>
                    </div>
                    <div class="col-md-6 mt-3">
                        <span class="text-danger">*</span>
                        <input type="text" class="form-control" placeholder="Enter your Email" name="Qu_email" required>
                    </div>
                    <div class="col-md-12 mt-3">
                        <span class="text-danger">*</span>
                        <textarea class="form-control textarea" name="Qu_query" rows="8" placeholder="Enter Your Query In Details" required></textarea>
                    </div>
                    <div class="col-md-12 mt-3 text-right">
                        <button class="btn d-md-none d-block btn-block btn-dark pl-5 pr-5">Submit</button>
                        <button class="btn d-none d-md-inline btn-dark pl-5 pr-5">Submit</button>
                    </div>
                </div>
            </form>
        </div>
    </div>
    <div class="row mt-5">
        <div class="col-md-4 " style="display: grid; place-items: center;">
            <div>
                <h1>Office Address : </h1>
                <h2>A2Z It Hub Pvt.</h2>
                <h3>Balikaashram Road</h3>
                <h2>Ahmednagar - 414001</h2>
            </div>
        </div>
        <div class="col-md-8">
            <iframe width="100%" height="400px" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d7255.387745191501!2d74.73022770000003!3d19.105346900000008!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x3bdcb17ca9d749e5%3A0x516744f9b2f35ec9!2sA2Z%20INFOTECH%20%26%20A2Z%20IT%20HUB%20PVT.%20LTD.!5e1!3m2!1sen!2sin!4v1694251769749!5m2!1sen!2sin" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
